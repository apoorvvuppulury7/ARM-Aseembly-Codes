  AREA LFSR, CODE, READONLY
START

  LDR R4, =0xE0028018;
  LDR R5, =0xE0028010;
  LDR R2, =0x000000FF;
  LDR R0, =0x000000AE;
  MOV R1, #0x00000000;
  MOV R3, #0X00000000;
  STR R2,[R4];

FEEDBACK_LOOP
    STR R0, [R5];
    EOR R1,R3,R0, LSR #3;
    EOR R1,R1,R0, LSR #4;
    EOR R1,R1,R0, LSR #5;
    EOR R1,R1,R0, LSR #7;
    AND R1, R1, #0x1;
    LSL R0, #0x1;
    ORR R0, R0, R1;
    B FEEDBACK_LOOP

  NOP
  END
